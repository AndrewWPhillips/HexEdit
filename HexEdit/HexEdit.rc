IDR_DFFD MENU 
BEGIN
    POPUP "Struct"
    BEGIN
        MENUITEM "E&xpand All",                 ID_DFFD_EXPANDALL
        MENUITEM SEPARATOR
        POPUP "&Insert"
        BEGIN
            MENUITEM "&Struct...",                  ID_DFFD_INSERT_STRUCT
            MENUITEM "&For...",                     ID_DFFD_INSERT_FOR
            MENUITEM "&If...",                      ID_DFFD_INSERT_IF
            MENUITEM "&Case...",                    ID_DFFD_INSERT_SWITCH
            MENUITEM "&Jump...",                    ID_DFFD_INSERT_JUMP
            MENUITEM SEPARATOR
            MENUITEM "Parse C/C++ Declarations...", ID_DFFD_PARSE_DECLARATIONS

            MENUITEM SEPARATOR
            MENUITEM "&Use Defined Struct...",      ID_DFFD_INSERT_USE_STRUCT

            MENUITEM "&Expression Evaluation...",   ID_DFFD_INSERT_EVAL
            MENUITEM "&Data...",                    ID_DFFD_INSERT_DATA
        END
        MENUITEM "&Delete...",                  ID_DFFD_DELETE
        POPUP "E&nclose in"
        BEGIN
            MENUITEM "&Struct...",                  ID_DFFD_ENCLOSE_STRUCT
            MENUITEM "&For...",                     ID_DFFD_ENCLOSE_FOR
            MENUITEM "&If...",                      ID_DFFD_ENCLOSE_IF
            MENUITEM "&Jump...",                    ID_DFFD_ENCLOSE_JUMP
        END
        MENUITEM SEPARATOR
        MENUITEM "&Edit...",                    ID_DFFD_EDIT
    END
    POPUP "Single"
    BEGIN
        MENUITEM "E&xpand All",                 ID_DFFD_EXPANDALL
        MENUITEM SEPARATOR
        POPUP "E&nclose in"
        BEGIN
            MENUITEM "&Struct...",                  ID_DFFD_ENCLOSE_STRUCT
            MENUITEM "&For...",                     ID_DFFD_ENCLOSE_FOR
            MENUITEM "&If...",                      ID_DFFD_ENCLOSE_IF
            MENUITEM "&Jump...",                    ID_DFFD_ENCLOSE_JUMP
        END
        MENUITEM SEPARATOR
        MENUITEM "&Edit...",                    ID_DFFD_EDIT
    END
    POPUP "RootNode"
    BEGIN
        MENUITEM "E&xpand All",                 ID_DFFD_EXPANDALL
        MENUITEM "&Disallow Editing",           ID_DFFD_VIEW_MODE
        MENUITEM SEPARATOR
        MENUITEM "&Save Template File",         ID_DFFD_SAVE
        MENUITEM "Save &As...",                 ID_DFFD_SAVEAS
        MENUITEM SEPARATOR
        MENUITEM "Add Struct Definition",       ID_DFFD_INSERT_DEFINE_STRUCT
        MENUITEM "Settings...",                 ID_DFFD_GLOBAL
        MENUITEM "&Edit...",                    ID_DFFD_EDIT
    END
    POPUP "ButtonInsert"
    BEGIN
        MENUITEM "&Struct...",                  ID_DFFD_INSERT_STRUCT
        MENUITEM "&For...",                     ID_DFFD_INSERT_FOR
        MENUITEM "&If...",                      ID_DFFD_INSERT_IF
        MENUITEM "&Case...",                    ID_DFFD_INSERT_SWITCH
        MENUITEM "&Jump...",                    ID_DFFD_INSERT_JUMP
